Heating Stage 1 - Constant Volume heating, 50 ps  0 -> 100 K
 &cntrl
  ! General Input Controls 
  imin=0,                   ! Run MD
  irest={restart_sim},                  ! Restart a new simulation
  ntx={read_prev_coordinates},                    ! Read only coordinates
  nstlim={steps},             ! 25,000 steps = 50 picoseconds
  dt={timestep},                 ! 2 fs timestep
  ig=-1,                    ! RNG thingy 

  ! Energy Function Controls 
  ntc={SHAKE_param},                    !
  ntf={Force_calculation},                    
  cut={nonbonded_cut},                  ! Nonbonded cutoff 
  ntr={restraint},                    ! Restraints present
  restraint_wt={max_force},          ! Restraing Weight and mask
  restraintmask=':1-RESNUM@CA,C,N,O,PA,PB,Mg,MG', ! Restraints on CA atoms only
 
  ! Temperature Regulation
  tempi={initial_temperature},                ! Starting Temperature
  temp0={final_temperature},              ! Target Temperature 
  ntt={thermostat},                                  ! Thermostat type 
  gamma_ln={Collision_frequency},         ! Compulsory with ntt=3
  tautp={heat_bath_coupling_constant}     ! Compulsory with ntt=1

  ! Volume Regulation 
  ntb={PBC_treatment},                    
  
  ! Output Controls 
  iwrap=1,                  ! Wrap atoms to primary box
  ntpr=2500,                 ! Energy write frequency 
  ntwx=2500,                 ! Coordinate Write Frequency (1 ps)
  ioutfm=1,                 ! Output Format
  ntxo=2,                   ! Restart File Format 
  nmropt=1,
 &end
 
 /
 &wt
   type='TEMP0', istep1=0, istep2=25000,
   value1={initial_temperature}, value2={final_temperature}
 /
 &wt type='END'
 /
